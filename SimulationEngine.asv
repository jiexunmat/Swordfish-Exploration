function [ responses ] = SimulationEngine( params,designVars )
%SIMULATIONENGINE 
% params = [L,X,Y,E]
% designVars = [w,t]    % width, thickness
% responses = [A,R,D]   % Area, Stress, Displacement


A = w*t;
R = 600*Y/(w*t^2) + 600*X/(w^2*t);
D = 4*L^3 / (E*w*t) * sqrt((Y/t^2)^2 + (X/w^2)^2);

responses = [A,R,D];
end

